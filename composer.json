{
    "name": "odolbeau/rabbit-mq-admin-toolkit",
    "description": "RabbitMQ administration toolkit",
    "keywords": ["rabbitmq", "admin", "toolkit", "amqp"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "8.*",
        "symfony/console": "^4.4|^5.3|^6.0",
        "symfony/yaml": "^4.4|^5.3|^6.0",
        "swarrot/swarrot": "^4.12",
        "ext-amqp": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Bab\\RabbitMq\\": "src/Bab/RabbitMq/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bab\\RabbitMq\\Tests\\": "tests/"
        }
    },
    "bin": ["rabbit"],
    "extra": {
        "branch-alias": {
            "dev-master": "5.2.x-dev"
        }
    }
}
